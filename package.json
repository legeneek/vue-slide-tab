{
  "name": "vue-slide-tab",
  "version": "2.0.0",
  "description": "vue slide tab for mobile",
  "main": "dist/vue-slide-tab.umd.js",
  "module": "dist/vue-slide-tab.esm.js",
  "unpkg": "dist/vue-slide-tab.js",
  "scripts": {
    "start": "webpack-dev-server --config ./build/webpack.config.js",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-slide-tab.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-slide-tab.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-slide-tab.js",
    "build:demo": "NODE_ENV=product webpack --config ./build/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/legeneek/vue-slide-tab.git"
  },
  "keywords": [
    "vue",
    "tab"
  ],
  "author": "legeneek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/legeneek/vue-slide-tab/issues"
  },
  "homepage": "https://github.com/legeneek/vue-slide-tab#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@rollup/plugin-buble": "^0.20.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-vue": "^5.1.2",
    "style-loader": "^0.23.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
